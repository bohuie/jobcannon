<%= javascript_include_tag "https://cdnjs.cloudflare.com/ajax/libs/d3/3.5.5/d3.min.js" %>
<%= javascript_include_tag "http://labratrevenge.com/d3-tip/javascripts/d3.tip.v0.6.3.js" %>
<div class = 'row'>
  <div class = 'col-md-12'>
    <ul class="nav nav-tabs" role="tablist">
      <li role="presentation" class="active" style = 'width: 20%'>
        <a href="#profile" aria-controls="profile" role="tab" data-toggle="tab">Profile</a>
      </li>
      <li role="presentation" style = 'width: 20%'>
        <a href="#personality" aria-controls="personality" role="tab" data-toggle="tab">personality</a>
      </li>
      <li role="presentation" style = 'width: 20%'>
        <a href="#experiences" aria-controls="experiences" role="tab" data-toggle="tab">experiences</a>
      </li>
      <li role="presentation" style = 'width: 20%'>
        <a href="#technology" aria-controls="technology" role="tab" data-toggle="tab">technology</a>
      </li>
    </ul>
    <div class="tab-content">
      <div role="tabpanel" class="tab-pane fade in active" id="profile">
        <%= render :partial => 'profile', :locals => {data: @profile} %>
      </div>
      <div role="tabpanel" class="tab-pane fade" id="personality">
        <%= render :partial => 'personality', :locals => {data: @personality} %>
      </div>
      <div role="tabpanel" class="tab-pane fade" id="experiences">
            This will be experience content
      </div>
      <div role="tabpanel" class="tab-pane fade" id="technology">
            This will be technology content
      </div>
    </div>
  </div>
</div>