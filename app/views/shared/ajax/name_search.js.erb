d3.selectAll(".node").select("rect").transition().style("fill", "#ccc");
d3.selectAll(".node")
	.each(function(d){
		var current = d3.select(this);
		var info = nodes[current.select("text").text()]
		if(info.name.indexOf("<%= escape_javascript @search.query%>") > -1){
			current.select("rect")
				.style("fill", "red");
			hideProfile(current);
		}
 	});
