d3.selectAll(".node").select("circle").transition().style("fill", "#ccc");
d3.selectAll(".node")
	.each(function(d){
		var current = d3.select(this);
		if(current.select("text").text().indexOf("<%= escape_javascript @search.query%>") > -1){
			current.select("circle").transition()
				.style("fill", "red");
			hideProfile(this);
		}
 	});
