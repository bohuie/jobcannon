				</div>
				<div class="tab-pane fade in content survey-tabs" id="presentation-skills">
					<div class="row">
						<div class ="col-lg-10" id ="presentation">
							<%= render 'shared/survey/tech/presentation_a' %>												
						</div>
					</div>
				</div>
				<div class="tab-pane fade in content survey-tabs" id="email-skills">
					<div class="row">
						<div class ="col-lg-10" id = "email">										
							<%= render 'shared/survey/tech/email_a' %>						
						</div>
					</div>
				</div>
				<div class="tab-pane fade in content survey-tabs" id="internet-skills">
					<div class="row">
						<div class ="col-lg-10" id="internet">												
							<%= render 'shared/survey/tech/internet_a' %>						
						</div>
					</div>
				</div>
				<div class="tab-pane fade in content survey-tabs" id="social-media-skills">
