						<div class ="col-lg-10">
							<%= render 'shared/survey/tech/advanced_computer' %>
						</div>
					</div>
				</div>
				<div class="tab-pane fade in content survey-tabs" id="multi-media-skills">
					<div class="row">
						<div class ="col-lg-10" id = "media">						
							<%= render 'shared/survey/tech/multi_media_a' %>						
						</div>
					</div>
				</div>
				<div class="tab-pane fade in content survey-tabs" id="word-processing-skills">
					<div class="row">
						<div class ="col-lg-10" id="word">						
							<%= render'shared/survey/tech/word_processing_a' %>						
						</div>
					</div>
				</div>
				<div class="tab-pane fade in content survey-tabs" id="spreadsheet-skills">
