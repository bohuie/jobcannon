						</div>
					</div>
				</div>
				<div class="tab-pane fade in content survey-tabs" id="online-skills">
					<div class="row">
						<div class ="col-lg-10" id = "online">						
							<%= render 'shared/survey/tech/online_a' %>												
						</div>
					</div>
				</div>
			</div> 
		</div>
	</div>
